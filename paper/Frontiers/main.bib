  author   = {Rodriguez, JM and Rodriguez-Rivas, J and Domenico, TD and V{\'a}zquez, J and Valencia, A and Tress, ML},
  title    = {APPRIS 2017: principal isoforms for multiple gene sets},
  journal  = {Nucleic Acids Res},
  year     = {2018},
